<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.iteso.pae.browser.BrowserController">
   <center>
      <AnchorPane prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <WebView id="wbWebView" fx:id="wbWebView" layoutY="45.0" prefHeight="555.0" />
            <Button id="btnPrevious" fx:id="btnPrevious" layoutX="29.0" layoutY="6.0" mnemonicParsing="false" onAction="#previous" text="Previous" />
            <Button id="btnNext" fx:id="btnNext" disable="true" layoutX="118.0" layoutY="6.0" mnemonicParsing="false" onAction="#next" text="Next" />
            <TextField id="tfDirecciones" fx:id="tfDirecciones" layoutX="391.0" layoutY="6.0" prefHeight="31.0" prefWidth="317.0" />
            <Button id="btnSearch" fx:id="btnSearch" layoutX="721.0" layoutY="6.0" mnemonicParsing="false" onAction="#search" text="Search" />
            <ChoiceBox id="cbHistorial" fx:id="cbHistorial" layoutX="230.0" layoutY="6.0" prefWidth="150.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
